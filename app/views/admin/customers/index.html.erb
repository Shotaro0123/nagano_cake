<h1>会員一覧</h1>
<% @customers.each do |customer| %>
 <div>
  <table>
   <tr>
    <th><p>会員ID</p></th>
    <th><p>氏名</p></th>
    <th><p>メールアドレス</p></th>
    <th><p>ステータス</p></th>
   </tr>
   <tr>
    <th><%= customer.id %></th>
    <th>
     <%= link_to admin_customer_path(customer.id) do %>
     <%= customer.last_name %>
     <%= customer.first_name %>
     <% end %>
    </th>
    <th>
     <%= customer.email %>
    </th>
    <th>

     <%= customer.is_deleted %></th>
   </tr>
  </table>
 </div>
<% end %>