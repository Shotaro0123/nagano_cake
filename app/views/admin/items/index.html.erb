<h1>商品一覧</h1>
<% @items.each do |item| %>
 <div>
  <p>商品ID</p>
  <p>商品名</p>
  <p>税抜価格</p>
   </tr>
   <tr>
    <th><%= item.id %></th>
    <th>
     <%= link_to admin_item_path(item.id) do %>
     <%= item.name %>
     <% end %>
    </th>
    <th><%= item.price %></th>
   </tr>
  </table>
 </div>
<% end %>
<%= paginate @items %>