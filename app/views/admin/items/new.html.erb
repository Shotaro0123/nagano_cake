<h1>商品新規登録</h1>
<%= form_with model: @item, url: admin_items_path  do |f| %>
 <span>商品画像</span>
 <%= f.file_field :image, accept: "image/*" %><br>
 <span>商品名</span>
 <%= f.text_field :name %><br>
 <span>商品説明</span>
 <%= f.text_area :introduction %><br>
 <span>税抜価格</span>
 <%= f.text_field :price %>
 <span>円</span><br>
 <%= f.submit '新規登録' %>
<% end %>