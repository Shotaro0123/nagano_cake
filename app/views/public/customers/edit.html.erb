<h1>会員情報編集</h1>
<%= form_with model: @customer, url:customers_information_edit_path  do |f| %>
<table>
 <tr>
  <th>氏名</th>
  <th>
   <%= f.text_field :last_name, autofocus: true %>
   <%= f.text_field :first_name %>
  </th>
 </tr>
 <tr>
  <th>フリガナ</th>
  <th>
   <%= f.text_field :last_name_kana %>
   <%= f.text_field :first_name_kana %>
  </th>
 </tr>
 <tr>
  <th>郵便番号</th>
  <th>
   <%= f.text_field :postal_code %>
  </th>
 </tr>
 <tr>
  <th>住所</th>
  <th>
   <%= f.text_field :address %>
  </th>
 </tr>
 <tr>
  <th>電話番号</th>
  <th>
   <%= f.text_field :telephone_number %>
  </th>
 </tr>
 <tr>
  <th>メールアドレス</th>
  <th>
   <%= f.text_field :email %>
  </th>
 </tr>
 <tr>
  <th></th>
  <th>
   <%= f.submit '編集内容を保存' %>
  </th>
  <th>
   <%= link_to "退会する",  customers_unsubscribe_path %>
  </th>
 </tr>
</table>
<% end %>