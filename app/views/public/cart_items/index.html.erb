<h1>ショッピングカート</h1>
 <div>
  <table>
   <thead>
    <tr>
     <th>商品名</th>
     <th>単価（税込）</th>
     <th>数量</th>
     <th>小計</th>
     <th></th>
    </tr>
   </thead>
   <tbody>
   <%= @cart_items.each do |cart_item| %>
    <tr>
     <th><%= cart_item.name %></th>
     <th><%= cart_item.price %></th>
     <th><%= cart_item.amount %></th>
     <th><%= cart_item.name %></th>
     <th> </th>
    </tr>
   <% end %>
   </tbody>
  </table>
 </div>
